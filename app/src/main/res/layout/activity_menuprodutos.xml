        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="28dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button18"
        android:layout_width="290dp"
        android:layout_height="50dp"
