    <Button
        android:id="@+id/button21"
        android:layout_width="290dp"
        android:layout_height="50dp"
        android:text="+ TOMATE  "
        android:textAlignment="textStart"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="317dp"
