<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_ultimo">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="417dp"
        android:layout_height="772dp"
        app:srcCompat="@drawable/img_ultimo"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/editTextText14"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:background="@drawable/btn_fundo"
        android:ems="10"
        android:inputType="text"
        android:text="Veja o recibo"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="23dp"
        tools:layout_editor_absoluteY="185dp" />

    <EditText
        android:id="@+id/editTextText15"
        android:layout_width="159dp"
        android:layout_height="50dp"
        android:background="@drawable/btn_fundo"
        android:ems="10"
        android:inputType="text"
        android:text="  Acompanhar"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="412dp" />

</androidx.constraintlayout.widget.ConstraintLayout>